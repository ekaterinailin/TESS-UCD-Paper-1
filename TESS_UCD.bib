	Eprint = {arXiv:astro-ph/0112241},
	Journal = {\apjl},
	Keywords = {Stars: Activity, Stars: Individual: Alphanumeric: 2MASSI J1237392+652615, stars: individual (2MASSI J1315309-264951, 2MASSW J0036159+182110), Stars: Low-Mass, Brown Dwarfs},
	Month = jan,
	Pages = {L89-L92},
	Title = {{2MASSI J1315309-264951: An L Dwarf with Strong and Variable H{$\alpha$} Emission}},
	Volume = 564,
	Year = 2002,
