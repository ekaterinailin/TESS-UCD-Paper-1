@phdthesis{Hilton2011phd,
	Abstract = {Flares are magnetic reconnection events in the atmospheres of cool stars that lead to a myriad of observed emission, including broadband optical flux increases that can be more than 100 times the quiescent flux of the star. We have obtained nearly 1000 hours of photometric monitoring of 39 dwarf stars, both magnetically active and inactive, with spectral types from M0 to M8, to measure their flare frequency distributions (FFDs - the frequency of flares of various energies). The quantitative algorithm we developed for flare identification makes no assumptions about flare shapes and uses the noise characteristics of the light curves to determine the minimum flare energy that can be detected. We identify 239 flares, from which we present the first FFD measurements for both inactive and active early-type stars; inactive, mid-type stars; and active, late-type stars. We also measure FFDs for active, mid-type M dwarfs that compare well with previous studies. A subset of 137 flares were determined to be classical type flares, which we use to investigate the rise times, peak magnitudes, decay times, and total energies. We adopt an analytic flare shape model, and then use the measured FFDs and measured distributions of rise and decay times to construct simulated light curves appropriate for stars with specified spectral types and magnetic activity levels. We use the Galactic stellar population model Galfast to generate volume-limited catalogs of stars in the Milky Way. By constructing light curves with the appropriate FFDs for each star in the catalog, we can model the flare rates for time-domain photometric surveys. The Galactic flare rate model was compared to flares identified in the Sloan Digital Sky Survey Stripe 82, allowing us to calibrate the model parameters. When applied to the survey characteristics of the Large Synoptic Survey Telescope (LSST), the model predicts that there will be several flares with u-band enhancements of more than 0.1 magnitudes in every 9.6 deg² full-frame exposure at low Galactic latitudes. The model further predicts that M dwarf flaring will cause ˜1% of LSST u-band images to contain an optical transient - a large flare on an M dwarf that is too faint to be seen in any filter during quiescence.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2011PhDT.......144H},
	Author = {{Hilton}, E.~J.},
	Date-Added = {2019-08-12 12:55:46 +0200},
	Date-Modified = {2019-08-12 12:55:46 +0200},
	School = {University of Washington},
	Title = {{The Galactic M Dwarf Flare Rate}},
	Year = 2011,
	Bdsk-File-1 = {}}

@article{Schmidt2007,
	Abstract = {We present the activity and kinematics of a representative volume-limited (20 pc) sample of 152 late-M and L dwarfs (M7-L8) photometrically selected from the Two Micron All Sky Survey (2MASS). Using new proper-motion measurements and spectrophotometric distance estimates, we calculate tangential velocities. The sample has a mean tangential velocity of <Vtan>=31.5 km s-1, a velocity dispersion of sigmatan=20.7 km s-1, and a maximum tangential velocity of Vtan=138.8 km s-1. These kinematic results are in excellent agreement with previous studies of ultracool dwarfs in the local solar neighborhood. Halpha emission, an indicator of chromospheric activity, was detected in 63 of 81 late-M dwarfs and in 16 of 69 L dwarfs examined. We find a lack of correlation between activity strength, measured by log(FHalpha/Fbol), and Vtan, although velocity distributions suggest that the active dwarfs in our sample are slightly younger than the inactive dwarfs. Consistent with previous studies of activity in ultracool dwarfs, we find that the fraction of Halpha-emitting objects per spectral type peaks at spectral type M7 and declines through mid-L dwarfs. Activity strength is similarly correlated with spectral type for types later than M7. Eleven dwarfs out of 150 show evidence of variability, ranging from small fluctuations to large flare events. We estimate a flare cycle of ~5% for late-M dwarfs and ~2% for L dwarfs. Observations of strong, variable activity on the L1 dwarf 2MASS J10224821+5825453 and an amazing flare event on the M7 dwarf 2MASS J1028404-143843 are discussed.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2007AJ....133.2258S},
	Author = {{Schmidt}, S.~J. and {Cruz}, K.~L. and {Bongiorno}, B.~J. and {Liebert}, J. and {Reid}, I.~N.},
	Date-Added = {2019-08-12 12:55:03 +0200},
	Date-Modified = {2019-08-12 12:55:03 +0200},
	Doi = {10.1086/512158},
	Eprint = {arXiv:astro-ph/0701055},
	Journal = {\aj},
	Keywords = {Galaxy: stellar content, solar neighborhood, stars: activity, stars: flare, stars: late-type, stars: low-mass, brown dwarfs},
	Month = may,
	Pages = {2258-2273},
	Title = {{Activity and Kinematics of Ultracool Dwarfs, Including an Amazing Flare Observation}},
	Volume = 133,
	Year = 2007,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1086/512158}}

@article{Hall2002,
	Abstract = {The object 2MASSI J1315309-264951 is an L3 dwarf with strong Halpha emission discovered in the course of a color-selected survey for active galactic nuclei using the Two Micron All Sky Survey (2MASS). The strength of its Halpha emission decreased by about a factor of 2 between two epochs separated by 137 days. This is the first time that variable Halpha emission has been reported in an L dwarf, and it is probably the first observation of an Halpha flare in an L dwarf. The value of log(LHalpha/Lbol)>-4.17 observed at the discovery epoch is larger than that of any other L dwarf, but it is comparable to that of 2MASSI J1237392+652615, the only reported T dwarf with Halpha emission. The observed variability indicates that the Halpha emission of 2MASSI J1315309-264951 is powered either by magnetic fields or by accretion in a binary system. Spectroscopic or narrowband Halpha monitoring of L and T dwarfs on timescales of hours to days would be the most useful step toward a better understanding of their Halpha emission mechanism(s).},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2002ApJ...564L..89H},
