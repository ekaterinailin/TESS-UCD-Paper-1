	Doi = {10.1051/0004-6361:20066488},
	Eprint = {arXiv:astro-ph/0610582},
	Journal = {\aap},
	Keywords = {X-rays: stars, stars: activity, stars: flare, stars: low-mass, brown dwarfs, stars: coronae, stars: individual: LP 412-31},
	Month = dec,
	Pages = {L35-L38},
	Title = {{Simultaneous optical and X-ray observations of a giant flare on the ultracool dwarf LP 412-31}},
	Volume = 460,
	Year = 2006,
